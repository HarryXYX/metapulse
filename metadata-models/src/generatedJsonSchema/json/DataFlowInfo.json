{
  "definitions" : {
    "DataFlowInfo" : {
      "description" : "Information about a Data processing flow",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "customProperties" : {
          "default" : { },
          "description" : "Custom property bag.",
          "type" : "object",
          "additionalProperties" : {
            "type" : "string"
          }
        },
        "externalUrl" : {
          "default" : null,
          "description" : "URL where the reference exist",
          "type" : "string"
        },
        "name" : {
          "description" : "Flow name",
          "type" : "string"
        },
        "description" : {
          "default" : null,
          "description" : "Flow description",
          "type" : "string"
        },
        "project" : {
          "default" : null,
          "description" : "Optional project/namespace associated with the flow",
          "type" : "string"
        },
        "created" : {
          "default" : null,
          "description" : "A timestamp documenting when the asset was created in the source Data Platform (not on DataHub)",
          "$ref" : "#/definitions/TimeStamp"
        },
        "lastModified" : {
          "default" : null,
          "description" : "A timestamp documenting when the asset was last modified in the source Data Platform (not on DataHub)",
          "$ref" : "#/definitions/TimeStamp"
        },
        "env" : {
          "default" : null,
          "description" : "Environment for this flow",
          "$ref" : "#/definitions/FabricType"
        }
      },
      "required" : [ "customProperties", "name" ]
    },
    "TimeStamp" : {
      "description" : "A standard event timestamp",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "time" : {
          "description" : "When did the event occur",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        },
        "actor" : {
          "default" : null,
          "description" : "Optional: The actor urn involved in the event.",
          "type" : "string"
        }
      },
      "required" : [ "time" ]
    },
    "FabricType" : {
      "description" : "Fabric group type",
      "enum" : [ "DEV", "TEST", "QA", "UAT", "EI", "PRE", "STG", "NON_PROD", "PROD", "CORP", "RVW", "PRD", "TST", "SIT", "SBX", "SANDBOX" ]
    }
  },
  "$ref" : "#/definitions/DataFlowInfo"
