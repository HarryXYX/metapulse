{
  "definitions" : {
    "DatasetProfile" : {
      "description" : "Stats corresponding to datasets",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "timestampMillis" : {
          "description" : "The event timestamp field as epoch at UTC in milli seconds.",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        },
        "eventGranularity" : {
          "default" : null,
          "description" : "Granularity of the event if applicable",
          "$ref" : "#/definitions/TimeWindowSize"
        },
        "partitionSpec" : {
          "default" : {
            "partition" : "FULL_TABLE_SNAPSHOT",
            "type" : "FULL_TABLE",
            "timePartition" : null
          },
          "description" : "The optional partition specification.",
          "$ref" : "#/definitions/PartitionSpec"
        },
        "messageId" : {
          "default" : null,
          "description" : "The optional messageId, if provided serves as a custom user-defined unique identifier for an aspect value.",
          "type" : "string"
        },
        "rowCount" : {
          "default" : null,
          "description" : "The total number of rows",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        },
        "columnCount" : {
          "default" : null,
          "description" : "The total number of columns (or schema fields)",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        },
        "fieldProfiles" : {
          "default" : null,
          "description" : "Profiles for each column (or schema field)",
          "type" : "array",
          "items" : {
            "$ref" : "#/definitions/DatasetFieldProfile"
          }
        },
        "sizeInBytes" : {
          "default" : null,
          "description" : "Storage size in bytes",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        }
      },
      "required" : [ "timestampMillis", "partitionSpec" ]
    },
    "TimeWindowSize" : {
      "description" : "Defines the size of a time window.",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "unit" : {
          "description" : "Interval unit such as minute/hour/day etc.",
          "$ref" : "#/definitions/CalendarInterval"
        },
        "multiple" : {
          "default" : 1,
          "description" : "How many units. Defaults to 1.",
          "type" : "integer",
          "minimum" : -2147483648,
          "maximum" : 2147483647
        }
      },
      "required" : [ "unit", "multiple" ]
    },
    "CalendarInterval" : {
      "enum" : [ "SECOND", "MINUTE", "HOUR", "DAY", "WEEK", "MONTH", "QUARTER", "YEAR" ]
    },
    "PartitionSpec" : {
      "description" : "A reference to a specific partition in a dataset.",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "partition" : {
          "description" : "A unique id / value for the partition for which statistics were collected,\ngenerated by applying the key definition to a given row.",
          "type" : "string"
        },
        "timePartition" : {
          "default" : null,
          "description" : "Time window of the partition, if we are able to extract it from the partition key.",
          "$ref" : "#/definitions/TimeWindow"
        },
        "type" : {
          "default" : "PARTITION",
          "description" : "Unused!",
          "$ref" : "#/definitions/PartitionType"
        }
      },
      "required" : [ "partition", "type" ]
    },
    "TimeWindow" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "startTimeMillis" : {
          "description" : "Start time as epoch at UTC.",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        },
        "length" : {
          "description" : "The length of the window.",
          "$ref" : "#/definitions/TimeWindowSize"
        }
      },
      "required" : [ "startTimeMillis", "length" ]
    },
    "PartitionType" : {
      "enum" : [ "FULL_TABLE", "QUERY", "PARTITION" ]
    },
    "DatasetFieldProfile" : {
      "description" : "Stats corresponding to fields in a dataset",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "fieldPath" : {
          "type" : "string"
        },
        "uniqueCount" : {
          "default" : null,
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        },
        "uniqueProportion" : {
          "default" : null,
          "type" : "number"
        },
        "nullCount" : {
          "default" : null,
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        },
        "nullProportion" : {
          "default" : null,
          "type" : "number"
        },
        "min" : {
          "default" : null,
          "type" : "string"
        },
        "max" : {
          "default" : null,
          "type" : "string"
        },
        "mean" : {
          "default" : null,
          "type" : "string"
        },
        "median" : {
          "default" : null,
          "type" : "string"
        },
        "stdev" : {
          "default" : null,
          "type" : "string"
        },
        "quantiles" : {
          "default" : null,
          "type" : "array",
          "items" : {
            "$ref" : "#/definitions/Quantile"
          }
        },
        "distinctValueFrequencies" : {
          "default" : null,
          "type" : "array",
          "items" : {
            "$ref" : "#/definitions/ValueFrequency"
          }
        },
        "histogram" : {
          "default" : null,
          "$ref" : "#/definitions/Histogram"
        },
        "sampleValues" : {
          "default" : null,
          "type" : "array",
          "items" : {
            "type" : "string"
          }
        }
      },
      "required" : [ "fieldPath" ]
    },
    "Quantile" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "quantile" : {
          "type" : "string"
        },
        "value" : {
          "type" : "string"
        }
      },
      "required" : [ "quantile", "value" ]
    },
    "ValueFrequency" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "value" : {
          "type" : "string"
        },
        "frequency" : {
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        }
      },
      "required" : [ "value", "frequency" ]
    },
    "Histogram" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "boundaries" : {
          "type" : "array",
          "items" : {
            "type" : "string"
          }
        },
        "heights" : {
          "type" : "array",
          "items" : {
            "type" : "number"
          }
        }
      },
      "required" : [ "boundaries", "heights" ]
    }
  },
  "$ref" : "#/definitions/DatasetProfile"
}