{
  "definitions" : {
    "BrowseResult" : {
      "description" : "The model for the result of a browse query",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "entities" : {
          "description" : "A list of entities under the queried path",
          "type" : "array",
          "items" : {
            "$ref" : "#/definitions/BrowseResultEntity"
          }
        },
        "groups" : {
          "default" : [ ],
          "description" : "A list of groups and total number of entities inside those groups under the queried path",
          "type" : "array",
          "items" : {
            "$ref" : "#/definitions/BrowseResultGroup"
          }
        },
        "metadata" : {
          "description" : "Metadata specific to the browse result of the queried path",
          "$ref" : "#/definitions/BrowseResultMetadata"
        },
        "from" : {
          "description" : "Offset of the first entity in the result",
          "type" : "integer",
          "minimum" : -2147483648,
          "maximum" : 2147483647
        },
        "pageSize" : {
          "description" : "Size of each page in the result",
          "type" : "integer",
          "minimum" : -2147483648,
          "maximum" : 2147483647
        },
        "numEntities" : {
          "description" : "The total number of entities directly under queried path",
          "type" : "integer",
          "minimum" : -2147483648,
          "maximum" : 2147483647
        },
        "numGroups" : {
          "description" : "The total number of groups directly under queried path",
          "type" : "integer",
          "minimum" : -2147483648,
          "maximum" : 2147483647
        },
        "numElements" : {
          "description" : "The total number of elements (entities + groups) directly under queried path",
          "type" : "integer",
          "minimum" : -2147483648,
          "maximum" : 2147483647
        }
      },
      "required" : [ "entities", "groups", "metadata", "from", "pageSize", "numEntities", "numGroups", "numElements" ]
    },
    "BrowseResultEntity" : {
      "description" : "Data model for an entity returned as part of a browse query",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "name" : {
          "default" : null,
          "description" : "Name of the entity",
          "type" : "string"
        },
        "urn" : {
          "description" : "URN of the entity",
          "type" : "string"
        }
      },
      "required" : [ "urn" ]
    },
    "BrowseResultGroup" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "name" : {
          "description" : "Name of the group",
          "type" : "string"
        },
        "count" : {
          "description" : "Number of entities that can be reached from this path",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        }
      },
      "required" : [ "name", "count" ]
    },
    "BrowseResultMetadata" : {
      "description" : "The model for browse result metadata",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "path" : {
          "description" : "Path that is being browsed",
          "type" : "string"
        },
        "totalNumEntities" : {
          "description" : "Total number of entities we can reach from path",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        }
      },
      "required" : [ "path", "totalNumEntities" ]
    }
  },
  "$ref" : "#/definitions/BrowseResult"
}