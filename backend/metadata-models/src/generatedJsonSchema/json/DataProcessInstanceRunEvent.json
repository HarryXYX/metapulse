{
  "definitions" : {
    "DataProcessInstanceRunEvent" : {
      "description" : "An event representing the current status of data process run.\nDataProcessRunEvent should be used for reporting the status of a dataProcess' run.",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "timestampMillis" : {
          "description" : "The event timestamp field as epoch at UTC in milli seconds.",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        },
        "eventGranularity" : {
          "default" : null,
          "description" : "Granularity of the event if applicable",
          "$ref" : "#/definitions/TimeWindowSize"
        },
        "partitionSpec" : {
          "default" : {
            "partition" : "FULL_TABLE_SNAPSHOT",
            "type" : "FULL_TABLE",
            "timePartition" : null
          },
          "description" : "The optional partition specification.",
          "$ref" : "#/definitions/PartitionSpec"
        },
        "messageId" : {
          "default" : null,
          "description" : "The optional messageId, if provided serves as a custom user-defined unique identifier for an aspect value.",
          "type" : "string"
        },
        "externalUrl" : {
          "default" : null,
          "description" : "URL where the reference exist",
          "type" : "string"
        },
        "status" : {
          "$ref" : "#/definitions/DataProcessRunStatus"
        },
        "attempt" : {
          "default" : null,
          "description" : "Return the try number that this Instance Run is in",
          "type" : "integer",
          "minimum" : -2147483648,
          "maximum" : 2147483647
        },
        "result" : {
          "default" : null,
          "description" : "The final result of the Data Processing run.",
          "$ref" : "#/definitions/DataProcessInstanceRunResult"
        },
        "durationMillis" : {
          "default" : null,
          "description" : "The duration of the run in milliseconds.",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        }
      },
      "required" : [ "timestampMillis", "partitionSpec", "status" ]
    },
    "TimeWindowSize" : {
      "description" : "Defines the size of a time window.",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "unit" : {
          "description" : "Interval unit such as minute/hour/day etc.",
          "$ref" : "#/definitions/CalendarInterval"
        },
        "multiple" : {
          "default" : 1,
          "description" : "How many units. Defaults to 1.",
          "type" : "integer",
          "minimum" : -2147483648,
          "maximum" : 2147483647
        }
      },
      "required" : [ "unit", "multiple" ]
    },
    "CalendarInterval" : {
      "enum" : [ "SECOND", "MINUTE", "HOUR", "DAY", "WEEK", "MONTH", "QUARTER", "YEAR" ]
    },
    "PartitionSpec" : {
      "description" : "A reference to a specific partition in a dataset.",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "partition" : {
          "description" : "A unique id / value for the partition for which statistics were collected,\ngenerated by applying the key definition to a given row.",
          "type" : "string"
        },
        "timePartition" : {
          "default" : null,
          "description" : "Time window of the partition, if we are able to extract it from the partition key.",
          "$ref" : "#/definitions/TimeWindow"
        },
        "type" : {
          "default" : "PARTITION",
          "description" : "Unused!",
          "$ref" : "#/definitions/PartitionType"
        }
      },
      "required" : [ "partition", "type" ]
    },
    "TimeWindow" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "startTimeMillis" : {
          "description" : "Start time as epoch at UTC.",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        },
        "length" : {
          "description" : "The length of the window.",
          "$ref" : "#/definitions/TimeWindowSize"
        }
      },
      "required" : [ "startTimeMillis", "length" ]
    },
    "PartitionType" : {
      "enum" : [ "FULL_TABLE", "QUERY", "PARTITION" ]
    },
    "DataProcessRunStatus" : {
      "enum" : [ "STARTED", "COMPLETE" ]
    },
    "DataProcessInstanceRunResult" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "type" : {
          "description" : " The final result, e.g. SUCCESS, FAILURE, SKIPPED, or UP_FOR_RETRY.",
          "$ref" : "#/definitions/RunResultType"
        },
        "nativeResultType" : {
          "description" : "It identifies the system where the native result comes from like Airflow, Azkaban, etc..",
          "type" : "string"
        }
      },
      "required" : [ "type", "nativeResultType" ]
    },
    "RunResultType" : {
      "enum" : [ "SUCCESS", "FAILURE", "SKIPPED", "UP_FOR_RETRY" ]
    }
  },
  "$ref" : "#/definitions/DataProcessInstanceRunEvent"
}