{
  "definitions" : {
    "PartitionsSummary" : {
      "description" : "Defines how the data is partitioned for Data Lake tables (e.g. Hive, S3, Iceberg, Delta, Hudi, etc).",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "minPartition" : {
          "default" : null,
          "description" : "The minimum partition as ordered",
          "$ref" : "#/definitions/PartitionSummary"
        },
        "maxPartition" : {
          "default" : null,
          "description" : "The maximum partition as ordered",
          "$ref" : "#/definitions/PartitionSummary"
        }
      }
    },
    "PartitionSummary" : {
      "description" : "Defines how the data is partitioned",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "partition" : {
          "description" : "A unique id / value for the partition for which statistics were collected,\ngenerated by applying the key definition to a given row.",
          "type" : "string"
        },
        "createdTime" : {
          "default" : null,
          "description" : "The created time for a given partition.",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        },
        "lastModifiedTime" : {
          "default" : null,
          "description" : "The last modified / touched time for a given partition.",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        }
      },
      "required" : [ "partition" ]
    }
  },
  "$ref" : "#/definitions/PartitionsSummary"
}