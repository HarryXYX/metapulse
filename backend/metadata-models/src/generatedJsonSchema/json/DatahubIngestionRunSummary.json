        "custom_summary" : {
          "default" : null,
          "description" : "Custom value.",
          "type" : "string"
        },
        "softwareVersion" : {
          "default" : null,
          "description" : "The software version of this ingestion.",
          "type" : "string"
        },
        "systemHostName" : {
