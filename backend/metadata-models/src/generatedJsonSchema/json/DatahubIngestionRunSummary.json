          "maximum" : 9223372036854775807
        },
        "numWorkUnitsCreated" : {
          "default" : null,
          "description" : "The number of workunits that are produced.",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
