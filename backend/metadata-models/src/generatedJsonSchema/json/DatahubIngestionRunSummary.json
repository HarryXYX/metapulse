    "PartitionType" : {
      "enum" : [ "FULL_TABLE", "QUERY", "PARTITION" ]
    },
    "JobStatus" : {
      "description" : "Job statuses",
      "enum" : [ "STARTING", "IN_PROGRESS", "STOPPING", "STOPPED", "COMPLETED", "FAILED", "UNKNOWN", "SKIPPED" ]
    }
  },
  "$ref" : "#/definitions/DatahubIngestionRunSummary"
}