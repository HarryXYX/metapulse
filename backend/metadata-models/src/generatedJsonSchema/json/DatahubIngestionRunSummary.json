          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        },
        "totalLatencySinkAPICalls" : {
          "default" : null,
          "description" : "Total latency across all sink API calls.",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        },
        "numWarnings" : {
          "default" : null,
          "description" : "Number of warnings generated.",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
