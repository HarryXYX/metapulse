        "availableMemory" : {
          "default" : null,
          "description" : "The available memory on the host the ingestion pipeline ran on.",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
