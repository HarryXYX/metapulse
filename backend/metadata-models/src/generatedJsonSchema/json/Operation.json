{
  "definitions" : {
    "Operation" : {
      "description" : "Operational info for an entity.",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "timestampMillis" : {
          "description" : "The event timestamp field as epoch at UTC in milli seconds.",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        },
        "eventGranularity" : {
          "default" : null,
          "description" : "Granularity of the event if applicable",
          "$ref" : "#/definitions/TimeWindowSize"
        },
        "partitionSpec" : {
          "default" : {
            "partition" : "FULL_TABLE_SNAPSHOT",
            "type" : "FULL_TABLE",
            "timePartition" : null
          },
          "description" : "The optional partition specification.",
          "$ref" : "#/definitions/PartitionSpec"
        },
        "messageId" : {
          "default" : null,
          "description" : "The optional messageId, if provided serves as a custom user-defined unique identifier for an aspect value.",
          "type" : "string"
        },
        "actor" : {
          "default" : null,
          "description" : "Actor who issued this operation.",
          "type" : "string"
        },
        "operationType" : {
          "description" : "Operation type of change.",
          "$ref" : "#/definitions/OperationType"
        },
        "customOperationType" : {
          "default" : null,
          "description" : "A custom type of operation. Required if operationType is CUSTOM.",
          "type" : "string"
        },
        "numAffectedRows" : {
          "default" : null,
          "description" : "How many rows were affected by this operation.",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        },
        "affectedDatasets" : {
          "default" : null,
          "description" : "Which other datasets were affected by this operation.",
          "type" : "array",
          "items" : {
            "type" : "string"
          }
        },
        "sourceType" : {
          "default" : null,
          "description" : "Source Type",
          "$ref" : "#/definitions/OperationSourceType"
        },
        "customProperties" : {
          "default" : null,
          "description" : "Custom properties",
          "type" : "object",
          "additionalProperties" : {
            "type" : "string"
          }
        },
        "lastUpdatedTimestamp" : {
          "description" : "The time at which the operation occurred. Would be better named 'operationTime'",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        },
        "queries" : {
          "default" : null,
          "description" : "Which queries were used in this operation.",
          "type" : "array",
          "items" : {
            "type" : "string"
          }
        }
      },
      "required" : [ "timestampMillis", "partitionSpec", "operationType", "lastUpdatedTimestamp" ]
    },
    "TimeWindowSize" : {
      "description" : "Defines the size of a time window.",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "unit" : {
          "description" : "Interval unit such as minute/hour/day etc.",
          "$ref" : "#/definitions/CalendarInterval"
        },
        "multiple" : {
          "default" : 1,
          "description" : "How many units. Defaults to 1.",
          "type" : "integer",
          "minimum" : -2147483648,
          "maximum" : 2147483647
        }
      },
      "required" : [ "unit", "multiple" ]
    },
    "CalendarInterval" : {
      "enum" : [ "SECOND", "MINUTE", "HOUR", "DAY", "WEEK", "MONTH", "QUARTER", "YEAR" ]
    },
    "PartitionSpec" : {
      "description" : "A reference to a specific partition in a dataset.",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "partition" : {
          "description" : "A unique id / value for the partition for which statistics were collected,\ngenerated by applying the key definition to a given row.",
          "type" : "string"
        },
        "timePartition" : {
          "default" : null,
          "description" : "Time window of the partition, if we are able to extract it from the partition key.",
          "$ref" : "#/definitions/TimeWindow"
        },
        "type" : {
          "default" : "PARTITION",
          "description" : "Unused!",
          "$ref" : "#/definitions/PartitionType"
        }
      },
      "required" : [ "partition", "type" ]
    },
    "TimeWindow" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "startTimeMillis" : {
          "description" : "Start time as epoch at UTC.",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        },
        "length" : {
          "description" : "The length of the window.",
          "$ref" : "#/definitions/TimeWindowSize"
        }
      },
      "required" : [ "startTimeMillis", "length" ]
    },
    "PartitionType" : {
      "enum" : [ "FULL_TABLE", "QUERY", "PARTITION" ]
    },
    "OperationType" : {
      "description" : "Enum to define the operation type when an entity changes.",
      "enum" : [ "INSERT", "UPDATE", "DELETE", "CREATE", "ALTER", "DROP", "CUSTOM", "UNKNOWN" ]
    },
    "OperationSourceType" : {
      "description" : "The source of an operation",
      "enum" : [ "DATA_PROCESS", "DATA_PLATFORM" ]
    }
  },
  "$ref" : "#/definitions/Operation"
}