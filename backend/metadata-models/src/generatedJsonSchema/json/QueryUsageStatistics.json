{
  "definitions" : {
    "QueryUsageStatistics" : {
      "description" : "Stats corresponding to dataset's usage.",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "timestampMillis" : {
          "description" : "The event timestamp field as epoch at UTC in milli seconds.",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        },
        "eventGranularity" : {
          "default" : null,
          "description" : "Granularity of the event if applicable",
          "$ref" : "#/definitions/TimeWindowSize"
        },
        "partitionSpec" : {
          "default" : {
            "partition" : "FULL_TABLE_SNAPSHOT",
            "type" : "FULL_TABLE",
            "timePartition" : null
          },
          "description" : "The optional partition specification.",
          "$ref" : "#/definitions/PartitionSpec"
        },
        "messageId" : {
          "default" : null,
          "description" : "The optional messageId, if provided serves as a custom user-defined unique identifier for an aspect value.",
          "type" : "string"
        },
        "queryCount" : {
          "default" : null,
          "description" : "Total query count in this bucket",
          "type" : "integer",
          "minimum" : -2147483648,
          "maximum" : 2147483647
        },
        "queryCost" : {
          "default" : null,
          "description" : "Query cost for this query and bucket",
          "type" : "number"
        },
        "lastExecutedAt" : {
          "default" : null,
          "description" : "Last executed timestamp",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        },
        "uniqueUserCount" : {
          "default" : null,
          "description" : "Unique user count",
          "type" : "integer",
          "minimum" : -2147483648,
          "maximum" : 2147483647
        },
        "userCounts" : {
          "default" : null,
          "description" : "Users within this bucket, with frequency counts",
          "type" : "array",
          "items" : {
            "$ref" : "#/definitions/DatasetUserUsageCounts"
          }
        }
      },
      "required" : [ "timestampMillis", "partitionSpec" ]
    },
    "TimeWindowSize" : {
      "description" : "Defines the size of a time window.",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "unit" : {
          "description" : "Interval unit such as minute/hour/day etc.",
          "$ref" : "#/definitions/CalendarInterval"
        },
        "multiple" : {
          "default" : 1,
          "description" : "How many units. Defaults to 1.",
          "type" : "integer",
          "minimum" : -2147483648,
          "maximum" : 2147483647
        }
      },
      "required" : [ "unit", "multiple" ]
    },
    "CalendarInterval" : {
      "enum" : [ "SECOND", "MINUTE", "HOUR", "DAY", "WEEK", "MONTH", "QUARTER", "YEAR" ]
    },
    "PartitionSpec" : {
      "description" : "A reference to a specific partition in a dataset.",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "partition" : {
          "description" : "A unique id / value for the partition for which statistics were collected,\ngenerated by applying the key definition to a given row.",
          "type" : "string"
        },
        "timePartition" : {
          "default" : null,
          "description" : "Time window of the partition, if we are able to extract it from the partition key.",
          "$ref" : "#/definitions/TimeWindow"
        },
        "type" : {
          "default" : "PARTITION",
          "description" : "Unused!",
          "$ref" : "#/definitions/PartitionType"
        }
      },
      "required" : [ "partition", "type" ]
    },
    "TimeWindow" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "startTimeMillis" : {
          "description" : "Start time as epoch at UTC.",
          "type" : "integer",
          "format" : "int64",
          "minimum" : -9223372036854775808,
          "maximum" : 9223372036854775807
        },
        "length" : {
          "description" : "The length of the window.",
          "$ref" : "#/definitions/TimeWindowSize"
        }
      },
      "required" : [ "startTimeMillis", "length" ]
    },
    "PartitionType" : {
      "enum" : [ "FULL_TABLE", "QUERY", "PARTITION" ]
    },
    "DatasetUserUsageCounts" : {
      "description" : "Records a single user's usage counts for a given resource",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "user" : {
          "description" : "The unique id of the user.",
          "type" : "string"
        },
        "count" : {
          "description" : "Number of times the dataset has been used by the user.",
          "type" : "integer",
          "minimum" : -2147483648,
          "maximum" : 2147483647
        },
        "userEmail" : {
          "default" : null,
          "description" : "If user_email is set, we attempt to resolve the user's urn upon ingest",
          "type" : "string"
        }
      },
      "required" : [ "user", "count" ]
    }
  },
  "$ref" : "#/definitions/QueryUsageStatistics"
}