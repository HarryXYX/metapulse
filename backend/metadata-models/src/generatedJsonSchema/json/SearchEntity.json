{
  "definitions" : {
    "SearchEntity" : {
      "description" : "The model for each entity returned by the search query",
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "entity" : {
          "description" : "Urn of the entity being returned",
          "type" : "string"
        },
        "matchedFields" : {
          "default" : [ ],
          "description" : "Matched field name and values",
          "type" : "array",
          "items" : {
            "$ref" : "#/definitions/MatchedField"
          }
        },
        "features" : {
          "default" : null,
          "type" : "object",
          "additionalProperties" : {
            "type" : "number"
          }
        },
        "score" : {
          "default" : null,
          "type" : "number"
        },
        "restrictedAspects" : {
          "default" : null,
          "description" : "A list of the the restricted aspects on the entity.\nIf the key aspect is present, assume ALL aspects should be restricted including the entity's Urn.",
          "type" : "array",
          "items" : {
            "type" : "string"
          }
        },
        "extraFields" : {
          "default" : null,
          "description" : "Extra fields from the search document based on what is requested in the search request",
          "type" : "object",
          "additionalProperties" : {
            "type" : "string"
          }
        }
      },
      "required" : [ "entity", "matchedFields" ]
    },
    "MatchedField" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "name" : {
          "description" : "Matched field name",
          "type" : "string"
        },
        "value" : {
          "description" : "Matched field value",
          "type" : "string"
        }
      },
      "required" : [ "name", "value" ]
    }
  },
  "$ref" : "#/definitions/SearchEntity"
}