      "/tags/*/attribution/source" : {
        "fieldName" : "fieldTagAttributionSources",
        "fieldType" : "URN",
        "queryByDefault" : false
      },
      "/tags/*/attribution/time" : {
        "fieldName" : "fieldTagAttributionDates",
        "fieldType" : "DATETIME",
        "queryByDefault" : false
      },
      "/tags/*/tag" : {
        "boostScore" : 0.5,
        "fieldName" : "fieldTags",
        "fieldType" : "URN"
      }
    }
  }, {
