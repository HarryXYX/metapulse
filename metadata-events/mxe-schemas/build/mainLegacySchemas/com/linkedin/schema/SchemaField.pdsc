        "queryByDefault" : false
      },
      "/terms/*/attribution/source" : {
        "fieldName" : "fieldTermAttributionSources",
        "fieldType" : "URN",
        "queryByDefault" : false
      },
      "/terms/*/attribution/time" : {
        "fieldName" : "fieldTermAttributionDates",
        "fieldType" : "DATETIME",
        "queryByDefault" : false
      },
      "/terms/*/urn" : {
        "boostScore" : 0.5,
        "fieldName" : "fieldGlossaryTerms",
        "fieldType" : "URN"
